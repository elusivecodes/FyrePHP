        "fyre/csp": "^7.0",
        "fyre/csrf": "^5.0",
        "fyre/db": "^6.0",
        "fyre/encryption": "^5.0",
        "fyre/engine": "^10.0",
        "fyre/entity": "^5.0",
        "fyre/error": "^6.0",
        "fyre/forge": "^5.0",
        "fyre/formatter": "^4.1",
        "fyre/lang": "5.0",
        "fyre/log": "^5.1",
        "fyre/mail": "^4.0",
        "fyre/make": "^2.0",
        "fyre/middleware": "^6.0",
        "fyre/migration": "^5.0",
        "fyre/orm": "^10.0",
        "fyre/queue": "^3.0",
        "fyre/router": "^7.0",
        "fyre/server": "^4.0",
        "fyre/session": "^5.0",
        "fyre/view": "^10.0",
        "fyre/timer": "^4.0",
        "fyre/iterator": "^5.0"
    },
    "require-dev": {
