{
    "name": "fyre/php",
    "description": "A PHP framework.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Elusive",
            "email": "elusivecodes@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require": {
        "fyre/auth": "^2.0",
        "fyre/cache": "^3.0",
        "fyre/command": "^4.0",
        "fyre/encryption": "3.0",
        "fyre/engine": "^9.0",
        "fyre/error": "^4.0",
        "fyre/loader": "^2.0",
        "fyre/log": "^3.0",
        "fyre/mail": "^2.0",
        "fyre/make": "^1.1",
        "fyre/migration": "^4.0",
        "fyre/orm": "^8.0",
        "fyre/queue": "^2.0",
        "fyre/session": "^4.0",
        "fyre/view": "^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.59",
        "fyre/php-cs-fixer-config": "^1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "chmod +x ./bin/fyre",
            "chmod +x ./bin/server"
        ],
        "cs": "php-cs-fixer fix --ansi --verbose --dry-run --diff",
        "cs-fix": "php-cs-fixer fix --ansi --verbose --diff"
    }
}